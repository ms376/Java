package DB.member;


public class Member { // DB table(member2)의 일치
	
	private String id; // id field (varchar2(20) : PK)
	private String pw; // pw field (varchar2(20))
	
	public String getId() {
		return id;
	}
	public String getPw() {
		return pw;
	}
	public void setId(String id) {
		this.id = id;
	}
	public void setPw(String pw) {
		this.pw = pw;
	}
}